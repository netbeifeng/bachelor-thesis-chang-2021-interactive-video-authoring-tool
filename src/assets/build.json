r":"Nil","wrongAnswers":["Amazonas","Donau","Mississippi"],"tip":"<br/> - Nil -> 6.671 Km\r\n            <br/> - Amazonas -> 6.400 Km\r\n            <br/> - Donau -> 2.850 Km\r\n            <br/> - Mississippi -> 3.730 Km","zIndex":1}],"customs":[{"transformations":[],"path":"./automaton.html","cid":402,"id":402,"startTime":30,"duration":10,"position":{"x":950,"y":110},"zIndex":1,"htmlContent":"<div class='customComponent'><script src=\"https://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js\"></script>\r\n<script src=\"https://netdna.bootstrapcdn.com/twitter-bootstrap/2.2.2/js/bootstrap.min.js\"></script>\r\n<script src=\"http://mdaines.github.io/viz.js/bower_components/viz.js/viz.js\"></script>\r\n<script src=\"https://cdn.jsdelivr.net/gh/izuzak/noam/lib/browser/noam.min.js\"></script>\r\n\r\n<style>\r\n    #container {\r\n        display: flex;\r\n        flex-wrap: nowrap;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        align-items: center;\r\n        /* position: absolute; */\r\n        border: 1px black solid;\r\n        padding: 25px;\r\n        /* left: 50%;\r\n        top: 50%;\r\n        transform: translate(-50%, -50%); */\r\n        border-radius: 15px;\r\n    }\r\n\r\n    #input {\r\n        width: 250px;\r\n        height: 400px;\r\n        font-size: 19px;\r\n        border-radius: 5px;\r\n    }\r\n\r\n    #text {\r\n        font-size: 18px;\r\n        border-radius: 5px;\r\n        margin-top: 15px;\r\n        margin-right: 15px;\r\n    }\r\n\r\n    button {\r\n        margin-top: 15px;\r\n        font-size: 18px;\r\n\r\n    }\r\n\r\n    #right {\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n</style>\r\n<div id=\"container\">\r\n    <div id=\"left\">\r\n        <p>Automaton: </p>\r\n        <textarea id=\"input\">\r\n#states\r\ns0\r\ns1\r\ns2\r\ns3\r\n#initial\r\ns0\r\n#accepting\r\ns3\r\n#alphabet\r\na\r\nb\r\nc\r\nd\r\n#transitions\r\ns0:a>s1\r\ns1:b>s2\r\ns2:c>s3\r\n        </textarea>\r\n        <br>\r\n        <button onclick=\"draw()\">Draw</button>\r\n    </div>\r\n    <div id=\"right\">\r\n        <div id=\"automatonGraph\">\r\n        </div>\r\n        <div style=\"display: flex; flex-direction: row; align-content: space-between;\">\r\n            <input id=\"text\" placeholder=\"abc\">\r\n            <button onclick=\"check()\">Check</button>\r\n        </div>\r\n        <span id=\"result\"></span>\r\n    </div>\r\n</div>\r\n<script>\r\n    setTimeout(() => { draw() }, 3000);\r\n    var automaton = null;\r\n    var currentStates = null;\r\n\r\n    function getElementsOfStates(states) {\r\n        var retVal = [];\r\n\r\n        for (var i = 0; i < states.length; i++) {\r\n            $(\"title:contains('\" + states[i].toString() + \"')\").each(function (index, element) {\r\n                if ($(this).text() === states[i].toString()) {\r\n                    retVal.push($(this).parent());\r\n                }\r\n            });\r\n        }\r\n\r\n        return retVal;\r\n    }\r\n\r\n    function draw() {\r\n        try {\r\n            automaton = noam.fsm.parseFsmFromString($(\"#input\").val());\r\n        } catch (e) {\r\n            console.error(\"ERROR BY PARSING AUTOMATON STRING: \" + e);\r\n        }\r\n\r\n        var stateElements = getElementsOfStates(automaton.acceptingStates);\r\n\r\n        for (var i = 0; i < stateElements.length; i++) {\r\n            var e1 = $(stateElements[i].children(\"ellipse\")[0]);\r\n            var e2 = $(stateElements[i].children(\"ellipse\")[1]);\r\n            e1.insertAfter(e2);\r\n        }\r\n\r\n        currentStates = noam.fsm.computeEpsilonClosure(automaton, [automaton.initialState]);\r\n        // console.log(automaton);\r\n        var dotString = noam.fsm.printDotFormat(automaton);\r\n        var gvizXml = Viz(dotString, \"svg\");\r\n        $(\"#automatonGraph\").html(gvizXml);\r\n        // reorderCirclesInAcceptingStates(automaton.acceptingStates);\r\n        $(\"#automatonGraph svg\").width($(\"#automatonGraph\").width());\r\n    }\r\n\r\n    function check() {\r\n        let flag = true;\r\n        currentStates = noam.fsm.computeEpsilonClosure(automaton, [automaton.initialState]);\r\n        let text = $(\"#text\").val();\r\n        let textArray = text.split('');\r\n        // console.log(textArray);\r\n        for (let [index, character] of textArray.entries()) {\r\n            if (automaton.alphabet.includes(character)) {\r\n                let beforeStates = currentStates;\r\n                // console.log(beforeStates);\r\n                currentStates = noam.fsm.makeTransition(automaton, currentStates, character);\r\n                // console.log(currentStates);\r\n                if (currentStates.length == 0) {\r\n                    console.error(\"Rejected in Pos \" + (index + 1) + \" with char \" + character + \" NOT ALLOWED FROM \" + beforeStates);\r\n                    $(\"#result\").html(\"Rejected in Pos \" + (index + 1) + \" with char \" + character + \" NOT ALLOWED FROM \" + beforeStates);\r\n                    $(\"#result\").attr(\"style\", \"color: firebrick\");\r\n                    flag = false;\r\n                    break;\r\n                }\r\n            } else {\r\n                console.error(\"Rejected Illegal Character \" + character);\r\n                $(\"#result\").html(\"Rejected Illegal Character \" + character);\r\n                $(\"#result\").attr(\"style\", \"color: firebrick\");\r\n                flag = false;\r\n            }\r\n        }\r\n        if (flag) {\r\n            let intersection = currentStates.filter(v => automaton.acceptingStates.includes(v));\r\n            if (intersection.length == 0) {\r\n                console.error(\"Rejected do not reach the acceptingStates, stop at \" + currentStates);\r\n                $(\"#result\").html(\"Rejected do not reach the acceptingStates, stop at \" + currentStates);\r\n                $(\"#result\").attr(\"style\", \"color: firebrick\");\r\n            } else {\r\n                console.log(\"Congurationlations! PASSED!\");\r\n                $(\"#result\").html(\"Congurationlations! PASSED!\");\r\n                $(\"#result\").attr(\"style\", \"color: forestgreen\");\r\n            }\r\n        }\r\n    }\r\n</script></div>"}],"graphics":[],"texts":[]},{"sid":5,"startTime":40,"duration":20,"name":"Animation Testing","page":5,"videos":[],"animations":[{"type":"cursor","startTime":40,"duration":1,"moveTo":{"x":100,"y":200}},{"type":"cursor","startTime":42,"duration":1,"moveTo":{"x":600,"y":200}},{"type":"cursor","startTime":44,"duration":1,"moveTo":{"x":600,"y":700}},{"type":"cursor","startTime":46,"duration":1,"moveTo":{"x":100,"y":700}},{"type":"cursor","startTime":48,"duration":1,"moveTo":{"x":100,"y":200}}],"images":[{"transformations":[{"elementType":"image","startTime":50,"duration":1,"elementId":501,"type":"scale","toPosition":null,"toScale":1.1},{"elementType":"image","startTime":52,"duration":1,"elementId":501,"type":"scale","toPosition":null,"toScale":0.8},{"elementType":"image","startTime":54,"duration":1,"elementId":501,"type":"move","toPosition":{"x":800,"y":200},"toScale":null},{"elementType":"image","startTime":56,"duration":1,"elementId":501,"type":"move","toPosition":{"x":800,"y":300},"toScale":null},{"elementType":"image","startTime":58,"duration":1,"elementId":501,"type":"move","toPosition":{"x":100,"y":200},"toScale":null}],"iid":501,"id":501,"startTime":40,"duration":20,"path":"https://homepages.cae.wisc.edu/~ece533/images/boat.png","isOnline":true,"position":{"x":100,"y":200},"height":500,"width":500,"zIndex":1}],"quizzes":[],"customs":[],"graphics":[],"texts":[]}],"title":"ILV Component Testing","course":"Bachelor Arbeit","chapter":"Inspection - Functional Examination","author":"Chang Luo","semester":"SoSe21","audio":"https://www2.cs.uic.edu/~i101/SoundFiles/CantinaBand60.wav","subtitle":"sub.vtt"}